						apiCall.call();
					}
				})); // --- slotRenderEnded

				// rewarded events +++
				googletag.pubads().addEventListener('rewardedSlotReady', D.closure( this, function(event) {
					if ( !this.isMyEvent( event.slot.getAdUnitPath() ) ) {
						return;
					}

					event.makeRewardedVisible();
