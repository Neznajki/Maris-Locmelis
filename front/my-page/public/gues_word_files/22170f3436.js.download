							}
							// if ( Math.random() < 0.5 ) {
							// 	D.Ads.processPassback2( adNode, adNode._placeId );
							// } else {
							// 	var inxAdNode = mkE({
							// 		tag: 'div',
							// 		id: 'page_top',
							// 		className: 'adv',
							// 		style: {
