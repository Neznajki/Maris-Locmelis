		_makeIdName: function( name ) {
			var element = document.getElementById( name );
			if ( !element ) {
				return name;
			}

			var count = 1;
			var newName = name + "--" + count;

			while ( document.getElementById( newName ) ) {
				count++;
				newName = name + "--" + count;
			}

			return newName;
