					apiCall.addMethod(this._mStats1);
					this._mStats2 = new DR.base.StatsDataAdd;
					this._mStats2.kw = 'ads_rewarded_ok_' + adNode._rewardedName;
					apiCall.addMethod(this._mStats2);
