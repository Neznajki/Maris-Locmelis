		if(v && D.DEV){
			var options = Style._OPTIONS[name];
			switch(v){
				case Style.INHERIT:
				case Style.INITIAL:
				case Style.UNSET:
					break;
				default:
					if(options && !Style.contains(options, v)){
						D.console.warn(name + ':' + JSON.stringify(v));
					}
			}
		}
		var names = [name];
		if(Style._WEBKIT[name]){
			names.push('-webkit-' + name);
		}
		if(Style._MS[name]){
			names.push('-ms-' + name);
		}
		if(Style._MOZ[name]){
			names.push('-moz-' + name);
		}
		if(Style._O[name]){
			names.push('-o-' + name);
		}
