/**
 * @param {RegExp} pattern
 * @param {String} subject
 * @param {Array} [matches]
 * @returns {Array|number}
 */
function preg_match_all( pattern, subject, matches ){
	var re = matches || [];
	if( !pattern.global ){
		D.console.error( pattern, 'Not global' );
		if(matches){
			return 0;
		}
		return re;
	}
	pattern.lastIndex = 0;
	var r;
	while( r = pattern.exec(subject) ){
		re.push(r);
	}
	if(matches){
		return re.length;
	}
	return re;
}

