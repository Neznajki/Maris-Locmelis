		D.onLoad( 'msg', function(){
			D.msg.newMail( {
				fid:fid,
				topic:topic,
				text:text,
				onSend:function(data){
					if(callback){
						callback(true, data);
					}
				},
				onClose:function(){
					if(callback){
						callback(false);
					}
				}
			} );
		} );
		return false;
	},

