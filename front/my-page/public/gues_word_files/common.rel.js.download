				}
			} catch(e){
			}
			var k = '_store_' + D.crypt( D.ID, D.ID );
			if(str){
				try{
					localStorage.setItem( k, str );
					return;
				} catch(e){
				}
				try{
					localStorage.clear();
					localStorage.setItem( k, str );
				} catch(e){
				}
				return;
