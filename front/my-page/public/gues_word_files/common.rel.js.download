	this._n = Style._n++;
	this.name = 'kf' + String(this._n);
	switch(typeof nameFrames){
		case 'string':
			if(nameFrames){
				this.name = nameFrames;
				this.nameCustom = true;
			}
			break;
		case 'object':
			this.frames = nameFrames;
			break;
	}
	if(frames){
		this.frames = frames;
	}
	this._onChange = D.closure(this, this._setCSS);
